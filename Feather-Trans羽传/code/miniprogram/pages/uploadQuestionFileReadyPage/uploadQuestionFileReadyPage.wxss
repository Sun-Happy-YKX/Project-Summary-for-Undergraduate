/* miniprogram/pages/uploadQuestionFileReadyPage/uploadQuestionFileReadyPage.wxss */
.bodyContainer {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  padding-bottom: 20px;
  margin-top: 20rpx;
}

.fileInfoContainer {
  width: 640rpx;
  border-radius: 25px;
  background-color: #99CCFF;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-bottom: 50rpx;
}

.fileInfoView {
  display: flex;
  justify-content: center;
  margin-top: 20px;
  padding-bottom: 20px;
}

.fileInfoText {
  margin-left: 50rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.titleText {
  font-weight: 800;
  color: #333333;
}

.nameInputBox {
  margin-bottom: 10px;
  width: 500rpx;
}

.questionNumSelectView {
  width: 640rpx;
  border-radius: 25px;
}

.selectHelpView {
  margin-left: 80rpx;
  margin-right: 80rpx;
}

.questionNumTipText {
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}

.custom-button {
  width: 26px;
  color: #fff;
  font-size: 10px;
  line-height: 18px;
  text-align: center;
  background-color:#2e76fc ;
  border-radius: 100px;
}

.setContentView {
  background-color: #F5F5DC;
  border-radius: 25px;
  padding: 20rpx 20rpx 20rpx 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 20rpx;
  margin-bottom: 12px;
}

.explainText {
  margin-top: 10rpx;
  font-size: 10px;
  color: #666666;
  font-weight: 400;
}

.setDateInputBox {
  width: 600rpx;
  text-align: center;
}

